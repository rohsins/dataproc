    "mocha": "^10.8.2",
    "ts-node": "^10.9.2"
  },
  "scripts": {
    "test": "mocha --exit ./_test_/test.spec.ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohsins/dataproc.git"
  },
  "bugs": {
    "url": "https://github.com/rohsins/dataproc/issues"
  },
  "description": "DataProc is a JavaScript library for transforming JSON object - flattening and unflattening.",
  "homepage": "https://github.com/rohsins/dataproc#readme",
  "keywords": [
    "flatten",
    "unflatten"
  ]
