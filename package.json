  "license": "MIT",
  "private": false,
  "main": "./dist/dataproc.cjs",
  "module": "./dist/dataproc.js",
  "types": "./dist/dataproc.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/dataproc.d.ts",
